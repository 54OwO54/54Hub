    end
    
    function ExpiredBloxyCola()
        game:GetService("ReplicatedStorage").RemoteEvents.GiveTool:FireServer("ExpiredBloxyCola")
    end

    function HealSelf()
        for count = 1, 300 do
            game:GetService("ReplicatedStorage").RemoteEvents.Energy:FireServer("Cat")
        end
        game:GetService("ReplicatedStorage").RemoteEvents.Cattery:FireServer()
    end

    function Plank()
        game:GetService("ReplicatedStorage").RemoteEvents.GiveTool:FireServer("Plank")
    end

