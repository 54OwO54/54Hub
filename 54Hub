            _G.HighJump = Value
            HighJump()
        end    
    })
    PlayerTab:AddButton({
        Name = "HealSelf",
        Callback = function()
            HealSelf()
          end    
    })
    local ToolTab = Window:MakeTab({
        Name = "Tools",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    ToolTab:AddButton({
        Name = "Apples",
        Callback = function()
